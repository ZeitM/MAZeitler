FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       regIOobject;
    location    "constant/polyMesh";
    object      cellZones;
    meta
    {
        names           6 ( arcZone6 arcZone5 arcZone4 arcZone3 arcZone2 arcZone1 );
    }
}
