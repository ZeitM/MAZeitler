FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            zeroGradient;
        value			uniform 0;
    }

    "outlet_ext*"
    {
        type            fixedValue;
        value           uniform 0;
    }
    "outlet_wall*|outlet_empty*"
    {
        type            slip;
    }    
    workpiece
    {
        type            zeroGradient;
        value			uniform 0;
    }
	
   "wall|chipWall|oversetWall"
   {
       type             zeroGradient;
       value			uniform 0;
   }
	
	outlet
    {
        type            zeroGradient;
        value			uniform 0;
    }
	
    tool
    {
        type            zeroGradient;
        value			uniform 0;
    }
	
    front
    {
        type            zeroGradient;
        value           uniform 0;
    }
	
    back
    {
        type            empty;
    }

    "chipOverset|overset2"
    {
        type            overset;
    }
}


